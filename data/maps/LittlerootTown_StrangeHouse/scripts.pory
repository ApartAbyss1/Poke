script LittlerootTown_StrangeHouse_Fridge {
	lockall
	msgbox("Heck yeah, two refrigerators!")
	releaseall
}

mapscripts StrangeHouse_MapScripts {
    MAP_SCRIPT_ON_TRANSITION: StrangeHouse_EventScript_BallDefault
}

script StrangeHouse_EventScript_BallDefault{
    setvar(STRANGEHOUSE_REGION, 0)
}

script StrangeHouse_EventScript_StrangePC{
    if(!(flag(STRANGEHOUSE_BALL_ONE) || flag(STRANGEHOUSE_BALL_TWO) || flag(STRANGEHOUSE_BALL_THREE))){
        lock
        faceplayer
        message("Where are you from?")
        multichoice(25, 0, MULTI_REGION_CHOICE, 0)
        closemessage
        if (var(VAR_RESULT) == 0){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 0)
        }
        elif (var(VAR_RESULT) == 1){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 1)
        }
        elif (var(VAR_RESULT) == 2){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 2)
        }
        elif (var(VAR_RESULT) == 3){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 3)
        }
        elif (var(VAR_RESULT) == 4){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 4)
        }
        elif (var(VAR_RESULT) == 5){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 5)
        }
        elif (var(VAR_RESULT) == 6){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 6)
        }
        elif (var(VAR_RESULT) == 7){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 7)
        }
        elif (var(VAR_RESULT) == 8){
            call(StarterFade)
            setvar(STRANGEHOUSE_REGION, 8)
        }
        release
    }
    else{
        msgbox("Take care of your Pok√©mon.", MSGBOX_NPC)
//        setflag(FLAG_SYS_POKEMON_GET)     // in case this script is used to replace birch's bag
    }
    end
}

script StarterFade{
    fadescreen(FADE_TO_BLACK)
    playfanfare(MUS_MOVE_DELETED)
    delay(10)
    fadescreen(FADE_FROM_BLACK)
    return
}

script StrangeHouse_EventScript_PokeballOne{
    lockall
    if(!(flag(STRANGEHOUSE_BALL_ONE) || flag(STRANGEHOUSE_BALL_TWO) || flag(STRANGEHOUSE_BALL_THREE))){
        switch(var(STRANGEHOUSE_REGION)){
            case 0: call(StrangeHouse_EventScript_Bulbasaur)
            case 1: call(StrangeHouse_EventScript_Chikorita)
            case 2: call(StrangeHouse_EventScript_Treecko)
            case 3: call(StrangeHouse_EventScript_Turtwig)
            case 4: call(StrangeHouse_EventScript_Snivy)
            case 5: call(StrangeHouse_EventScript_Chespin)
            case 6: call(StrangeHouse_EventScript_Rowlet)
            case 7: call(StrangeHouse_EventScript_Grookey)
//            case 8: call(StrangeHouse_EventScript_Sprigatito)
        }
   }
    else{
        msgbox("Don't take another.")
    }
    waitmessage
    releaseall
    end
}

